{
    "manifest_version": 2,

    "name": "momentum-image-downloader",
    "description": "下载 momentum 的图片",
    "version": "0.1",

    "browser_action": {
        "default_icon": "/icon.png",
        "default_popup": "/popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "downloads"
    ]
}
